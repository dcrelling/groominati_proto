<ul rn-carousel rn-carousel-control rn-carousel-index="index" class="image">
    <li ng-repeat="post in posts">
        <div class="row">
            <div class="col-md-6">
                <img ng-src="{{post.original}}">
            </div>
            <div class="col-md-6">
                <div class="row">
                    <div class="col-md-12">{{post.user_id}}</div>
                </div>
                <div class="row">
                    <div class="col-md-12">{{post.content}}</div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <ul>
                            <li ng-repeat="tag in post.tag_list">
                                <span class="groominati-tag-item">{{tag}}</span>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </li>
</ul>


